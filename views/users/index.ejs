<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>

    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Welcome to fazbook</h1>
    <a href='/users/new'>create a new user</a>
    <% users.forEach(function(user) { %>
        <div>
          <a href="<%= '/users/'+user.id %>"><span><%= user.firstName %></span></a>
        </div>
    <% }) %>
  </body>
</html>
